{
  "name": "@financial-project/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "typeorm": "ts-node --project apps/api/tsconfig.app.json node_modules/typeorm/cli.js",
    "assign-categories": "ts-node --project apps/api/tsconfig.app.json apps/api/src/scripts/assign-categories/index.ts",
    "assign-companies": "ts-node --project apps/api/tsconfig.app.json apps/api/src/scripts/assign-companies/index.ts",
    "generate-mcc-data": "ts-node --project apps/api/tsconfig.app.json apps/api/src/scripts/generate-mcc-data/index.ts",
    "seeds:run": "ts-node --project apps/api/tsconfig.app.json apps/api/src/seeds/index.ts",
    "migration:create": "npm run typeorm migration:create apps/api/src/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run -d apps/api/src/app/configs/typeorm.config.ts",
    "migration:initial": "npm run typeorm -- -d apps/api/src/app/configs/typeorm.config.ts migration:generate -n ./initial",
    "migration:generate": "npm run typeorm -- -d apps/api/src/app/configs/typeorm.config.ts migration:generate apps/api/src/migrations/$npm_config_name",
    "prepare": "npx husky && echo \"npx --no-install lint-staged\" > .husky/pre-commit",
    "docker:build": "docker build -t budget-api ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*{.js,.ts,.html}": "nx affected:lint --fix=true",
    "*": "nx format:write"
  },
  "private": true,
  "dependencies": {
